<h3><%= @television_show.title %></h3>

<ul>
  <li>Network: <%= @television_show.network %></li>
  <li>Years: <%= @television_show.years %></li>
</ul>

<div>
  <%= @television_show.synopsis %>
</div>

<div>
  <% @television_show.characters.each do |character| %>
      <%= " Character: #{character.name} Actor: #{character.actor} Description: #{character.description} " %>
    <li><%= link_to "Edit", edit_character_path(character) %></li>
            <li><%= link_to 'Delete', character, method: :delete, data: { confirm: 'Are you sure?' } %></li>
    </br>
  <% end %>
</div>
